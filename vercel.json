{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "LINE_CHANNEL_ACCESS_TOKEN": "@line_channel_access_token",
    "LINE_CHANNEL_SECRET": "@line_channel_secret",
    "GOOGLE_SHEET_ID": "@google_sheet_id",
    "GOOGLE_SERVICE_ACCOUNT_JSON": "@google_service_account_json"
  }
}
